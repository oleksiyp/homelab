{
  "manifest_version": 3,
  "name": "Earth meter",
  "version": "1.0.0",
  "description": "Application to count walking distances",
  "author": "Oleksii Pylypenko",
  "license": "MIT",
  "start_url": "/earth",
  "display": "standalone",
  "background_color": "#ffffff",
  "theme_color": "#317EFB",
  "orientation": "portrait-primary",
  "icons": [
    {
      "src": "/icon64.png",
      "sizes": "64x64",
      "type": "image/png"
    },
    {
      "src": "/icon128.png",
      "sizes": "128x128",
      "type": "image/png"
    },
    {
      "src": "/icon256.png",
      "sizes": "256x256",
      "type": "image/png"
    }
  ],
  "scope": "/earth",
  "categories": ["utilities", "productivity"],
  "lang": "en-US"
}
